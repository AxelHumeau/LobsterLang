fn fibonacci(| x |) {
    if x == 0 {
        0
    } else if x == 1 {
        1
    } else {
        fibonacci(| x - 1 |) + fibonacci(| x - 2 |)
    }
}

fibonacci(| 1 |)
fibonacci(| 2 |)
